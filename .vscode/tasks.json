{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "tymbuild",
            "command": "/usr/bin/gcc-13",
            "dependsOn": [
                "make make"
            ],
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "libft.a",
                "-o",
                "${fileDirname}/debugfile"
